---
import BaseLayout from "@/layouts/BaseLayout.astro";

import HeroTwo from "@/components/headers/HeroTwo.astro";
import LogoCloudTwo from "@/components/testimonials/LogoCloudTwo.astro";
import FeatureSeven from "@/components/features/FeatureSeven.astro";
import FeatureFive from "@/components/features/FeatureFive.astro";
import FeatureFour from "@/components/features/FeatureFour.astro";
import FeatureTwo from "@/components/features/FeatureTwo.astro";
import TestimonialsTwo from "@/components/testimonials/TestimonialsTwo.astro";
import PricingTwo from "@/components/pricing/PricingTwo.astro";
import FaqThree from "@/components/infopages/FaqThree.astro";

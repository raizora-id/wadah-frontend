{
  "name": "@rac/frontend-monorepo",
  "private": true,
  "scripts": {
    "dev:erp": "pnpm --filter @rac/erp dev",
    "dev:zycas": "pnpm --filter @rac/zycas dev",
    "dev:zycas-compro": "pnpm --filter @rac/zycas-compro dev",
    "build:erp": "pnpm --filter @rac/erp build",
    "build:zycas": "pnpm --filter @rac/zycas build",
    "build:zycas-compro": "pnpm --filter @rac/zycas-compro build",
    "build": "pnpm --filter [nama-project] build",
    "start": "pnpm --filter [nama-project] start",
    "lint": "pnpm --filter [nama-project] lint",
    "update-deps": "npm-check-updates --configFileName .ncurc.json"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@moonrepo/cli": "^1.33.2",
    "@t3-oss/env-nextjs": "^0.11.1",
    "npm-check-updates": "^17.1.14"
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0"
  },
  "packageManager": "pnpm@9.12.3"
}

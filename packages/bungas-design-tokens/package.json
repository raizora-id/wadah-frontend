        "prepublishOnly": "yarn build",
        "clean": "del-cli dist output src/js/createTokens.* src/js/defaultFoundation.*"
    },
    "author": "lab@raizora.com",
    "keywords": [
        "bungas",
        "ui",
        "design",
        "tokens"
    ],
    "devDependencies": {
        "@types/lodash": "^4.17.7",
        "figx": "^0.1.3",
        "lodash": "^4.17.21",
        "memfs": "^3.5.1",
        "style-dictionary": "^3.8.0",
        "xml": "^1.0.1"
    },
    "dependencies": {
        "color2k": "^2.0.3"
    }
}